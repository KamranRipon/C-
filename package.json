{
  "name": "cci",
  "version": "1.0.0",
  "description": "CCI - Chatbot Control Interface",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "delete:reports": "rm -r cypress/results/* && rm -r ./output.html || true",
    "prereport": "npm run delete:reports",
    "cy:run_spec": "npx cypress run --spec 'cypress/integration/End2EndTest.spec.js'",
    "cypress:run": "npm run cy:run_spec -- --browser electron",
    "CyTest": "npm run prereport && npm run cypress:run"
  },
